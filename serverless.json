{
  "custom": {
    "dotenv": {
      "exclude": ["AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY"]
    }
  },
  "frameworkVersion": "3",
  "functions": {
    "default": {
      "events": [{ "httpApi": "*" }],
      "fileSystemConfig": {
        "arn": "arn:aws:elasticfilesystem:us-east-1:004377108014:access-point/fsap-040fac057e3a54fa9",
        "localMountPath": "/mnt/efs"
      },
      "handler": "app/lambda.handler",
      "vpc": {
        "securityGroupIds": ["sg-1bdcf06a"],
        "subnetIds": [
          "subnet-a0c62ac4",
          "subnet-9b43dbb7",
          "subnet-d45c0e9c",
          "subnet-7407972e",
          "subnet-54a6a968",
          "subnet-c0a4cecc"
        ]
      }
    }
  },
  "package": {
    "patterns": ["app/**", "node_modules/**"]
  },
  "plugins": ["serverless-dotenv-plugin", "serverless-offline"],
  "provider": {
    "architecture": "x86_64",
    "name": "aws",
    "runtime": "nodejs14.x"
  },
  "service": "wilsjs"
}
